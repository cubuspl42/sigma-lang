%class HtmlElement ^{
    tagName: String,
    children: ^[],
}

%class TextNode ^{
    content: String,
}

%meta Node = HtmlElement.type | TextNode.type

%const node: Node = HtmlElement.new{
    tagName: "div",
    children: [],
}

%const main = %if is[node, HtmlElement] (
    %then { htmlElement: node },
    %else { textNode: node }
)
