let {
    N = 100,

    sumOfSquares = n => if[{
        test = eq[{
            left = n,
            right = 1,
        }],
        then = 1,
        else = plus[{
            left = sumOfSquares[
                minus[{
                    left = n,
                    right = 1,
                }]
            ],
            right = sq[n],
        }],
    }],

    sumOf = n => if[{
        test = eq[{
            left = n,
            right = 1,
        }],
        then = 1,
        else = plus[{
            left = sumOf[
                minus[{
                    left = n,
                    right = 1,
                }]
            ],
            right = n,
        }],
    }],

    squareOfSum = n => let {
        s = sumOf[n],
    } in sq[s],

    sumOfSquaresN = sumOfSquares[N],

    squareOfSumN = squareOfSum[N],

} in minus[{
    left = squareOfSumN,
    right = sumOfSquaresN,
}]
