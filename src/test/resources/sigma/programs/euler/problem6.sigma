let {
    N: Int = 100,

    sumOfSquares = n => if[n == 1] {
        then = 1,
        else = add{
            augend = sumOfSquares[n - 1],
            addend = sq[n],
        },
    },

    sumOf = n => if [n == 1] {
        then = 1,
        else = sumOf[n - 1] + n,
    },

    squareOfSum = n => let {
        s = sumOf[n],
    } in sq[s],

} in squareOfSum[N] - sumOfSquares[N]
