%import cat

%class Dog {
    %constructor of ^{}

    %fun bark ^{} => "Woof!"
}

%val pussy = cat.Cat.of{
}

%val rex = Dog.of{
}

%val letters = ["a", "b", "c", "d"]

%val emptiness = []

%val thing = letters

%val main = %let {
    result = %match thing (
        [head, ...tail] => tail
        [] => "Empty!"
        cat.Cat %as c => cat.Cat.meow{this = c}
        Dog %as d => Dog.bark{this = d}
    )
} %in result
